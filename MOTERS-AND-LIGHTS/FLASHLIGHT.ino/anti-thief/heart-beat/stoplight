void setup()
{
 pinMode(9,OUTPUT); //ALLOWS 13 TO ABSORB A LOT OF AMPS
}
void loop()
{
 analogWrite(9,255);
 delay(1000);
  analogWrite(9,180);
  delay(1000);
  analogWrite(9,64);
 delay(1000);
  analogWrite(9,32);
 delay(1000);
  analogWrite(9,16);
 delay(1000);
  analogWrite(9,8);
 delay(1000);
  analogWrite(9,255);
 delay(1000);
}
